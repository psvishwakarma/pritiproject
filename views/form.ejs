<body>

  <div class="container-scroller">

    <%- include('includes/navbar'); %>

      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_sidebar.html -->
        <%- include('includes/sideBar'); %>
          <div class="main-panel">
            <div class="content-wrapper">
              <div class="page-header px-3">
                <h1 class="page-title">Vendor Setup Form</h1>
            </div>
               <div class="container">
                <!-- <div class="Vendor">

                <h3>Vendor Setup</h3>

              </div> -->

                

              <div class=" col-lg col-md-4 col-sm-12 mt-3">

                <!--Personal Information:  -->

                 <div class=" card">

                  <div class="card-body">

                    <div class="row ">

                      <h6>Personal Information<span>:</span></h6>

                      <div class="col-12 col-sm-4">

                        <div class="">

                          <label for="floatingFname">First Name<span>*</span></label>

                          <input type="text" class="form-control" id="floatingFname" autocomplete="false" minlength="5"
                            maxlength="15" required>

                        </div>

                      </div>

                      <div class="col-12 col-sm-4">

                        <div class="">

                          <label for="floatingLname">Last Name<span>*</span></label>

                          <input type="text" class="form-control" id="floatingLname" autocomplete="false" minlength="5"
                            maxlength="15" required>

                        </div>

                      </div>

                      <div class="col-12 col-sm-4">

                        <div class="">

                          <label for="floatingInput">Company Name<span>*</span></label>

                          <input type="text" class="form-control" id="floatingname" autocomplete="false" minlength="3"
                            maxlength="50" required>

                        </div>

                      </div>

                      
                      <div class="col-12 col-sm-6 ">

                        <label for="floatingCategure">Category<span>*</span></label>

                        <select class="form-select" aria-label="Default select example">
                          <option selected>Contract</option>
                          <option value="1">Lease</option>
                          <option value="2">Supplier</option>
                          <option value="3"></option>
                        </select>

                      </div>

                      <div class="col-12  col-md-6">
                        
                          <label for="floatingType">Type<span>*</span></label>
                           <div class="row">
                          <div class="col-sm-4">
                            <input type="radio" id="contactChoice1" name="contact" value="Individual" />
                            <label for="contactChoice1">Individual</label>
                          </div>
                          <div class="col-sm-4">
                            <input type="radio" id="contactChoice2" name="contact" value="Company" />
                            <label for="contactChoice2">Company</label>
                          </div>
                          </div>
                        </div>

                      </div>
                    </div>



                  </div>

                </div> 

              
               <div class=" col-lg col-md-4 col-sm-12 mt-3"> 
              <!--Classification  -->

               <div class=" card">

                <div class="card-body">

                  <div class="row ">

                    <h6>Classification<span>:</span></h6>

                    <div class="col-12 col-sm-6">

                      <div class="">

                        <label for="floatingSsidiary">Subsidiary<span>*</span></label>

                        <select class="form-control" id="floatingSsidiary" fdprocessedid="rdsil5">

                          <option>India</option>

                          <option>Usa </option>

                        </select>

                      </div>

                    </div>


                    <div class="col-12 col-sm-6">
                      <div class="">

                        <label>Term's</label>

                        <select class="form-select" aria-label="Default select example">
                          <option selected>Net30</option>
                          <option value="1">Net60</option>
                          <option value="2">Net90</option>
                        </select>

                      </div>

                    </div>

                  </div>

                </div>

              </div></div> 

               <div class=" col-lg col-md-4 col-sm-12 mt-3"> 
              <!-- Contact Information: -->

              <div class=" card">

                <div class="card-body">
<div class="row">

                    <h6>Contact Information<span>:</span></h6>
                    

                    <div class="col-lg  col-sm-4">

                      <div class="">

                        <label for="floatingPemail">Email<span>*</span></label>

                        <input type="email" class="form-control" id="floatingPemail" autocomplete="false">

                      </div>

                    </div>



                    <div class="col-lg   col-sm-4">

                      <div class="">

                        <label for="floatingPcontact">Contact<span>*</span></label>

                        <input type="text" class="form-control" id="floatingPcontact" autocomplete="false">

                      </div>
                      </div>
                      <div class="col-12 col-sm-4 mt-3 py-3">

                        <div class="row">
                          <button class="btn btn-primary" type="submit" fdprocessedid="s9c2j">add</button>
                        </div>
                        </div>
                        </div>

                    </div>


                  </div>

                </div>

              
              

              <div class=" col-lg col-md-4 col-sm-12 mt-3">
              <!---Address--->
              <div class=" card">

                <div class="card-body">

                  <div class="row ">

                    <h6>Address<span>:</span></h6>

                    <div class="col-12 col-sm-4">

                      <div class="">

                        <label for="floatingstreet1">Street Address<span>*</span></label>

                        <input type="text" class="form-control" id="floatingstreet1" autocomplete="false" minlength="5"
                          maxlength="15" required>

                      </div>

                    </div>

                    <div class="col-12 col-sm-4">

                      <div class="">

                        <label for="floatingstreet2">Street Address2<span>*</span></label>

                        <input type="text" class="form-control" id="floatingstreet2" autocomplete="false" minlength="5"
                          maxlength="15" required>

                      </div>

                    </div>

                    <div class="col-12 col-sm-4">

                      <div class="">

                        <label for="floatingcity">City<span>*</span></label>

                        <input type="text" class="form-control" id="floatingcity" autocomplete="false" minlength="3"
                          maxlength="50" required>

                      </div>

                    </div>

                    <div class="col-12 col-sm-4">

                      <div class="">

                        <label for="floatingstate">State/Province<span>*</span></label>

                        <input type="text" class="form-control" id="floatingstate" autocomplete="false" minlength="3"
                          maxlength="50" required>

                      </div>

                    </div>

                    <div class="col-12 col-sm-4">

                      <label for="floatingpostal">Postal/Zip code<span>*</span></label>
                      <input type="text" class="form-control" id="floatingpostal" autocomplete="false" minlength="3"
                        maxlength="50" required>


                    </div>

                    <div class="col-12 col-sm-4">

                      <label for="floatingCategure">Country<span>*</span></label>

                      <select class="form-select" aria-label="Default select example">
                        <option selected>Please Select </option>
                        <option value="1">India</option>
                        <option value="2">USA</option>
                      </select>

                    </div>

                  </div>

                </div>

              </div>
              </div>
              </div>
              
              

              <div class="col-12 text-center my-3">
                <button class="btn btn-primary" type="submit" id="save">save</button>
              </div>
            </div> 
            
